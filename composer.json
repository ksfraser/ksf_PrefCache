{
    "name": "ksfraser/pref-cache",
    "description": "Lightweight, provider-agnostic preference caching library for PHP",
    "type": "library",
    "keywords": ["cache", "preferences", "configuration", "performance"],
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "ksfraser",
            "email": "ksfraser@users.noreply.github.com"
        }
    ],
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "KSF\\PrefCache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KSF\\PrefCache\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
